package tw.org.iii.tutor;

public class Thead {

	public static void main(String[] args) {
		int i = 1;
		while (true) {
			System.out.println(i);
			i += 2;
			if (i > 9) {
				break;
			}
		}
		System.out.println("-------");
		int j = 2;
		while (true) {
			System.out.println(j);
			j += 2;
			if (j > 8) {
				break;
			}
		}
		

	}
}
